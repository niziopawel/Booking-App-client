{
    "extends": "rallycoding",
    "plugins": ["babel"],
    "rules": {
        "arrow-body-style": 0,
        "object-shorthand": 0,
        "no-undef":0,
        "global-require": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 0,
        "radix": 0,
        "max-len": 0,
        "no-useless-escape": 0
    }
}